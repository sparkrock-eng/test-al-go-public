{
  "type": "AppSource App",
  "templateUrl": "https://github.com/microsoft/AL-Go-AppSource@main",
  "templateSha": "8d285b61b93664773272a886ffd0ba0e6da28846",
  "rulesetFile": "../sr365.ruleset.json",
  "useProjectDependencies": false,
  "alwaysBuildAllProjects": false,
  "enableCodeCop": true,
  "enableUICop": true,
  "enableAppSourceCop": true,
  "failOn": "error",
  "cacheKeepDays": 10,
  "doNotBuildTests": false,
  "doNotRunTests": false,
  "doNotSignApps": false,
  "BcContainerHelperVersion": "preview",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*"
  ],
  "CICDPullRequestBranches": [
    "main"
  ],
  "skipUpgrade": false,
  "ConditionalSettings": [
    {
      "branches": [
        "feature/*"
      ],
      "settings": {
      "doNotSignApps": true,
      "doNotRunTests": true
      }
    },
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "doNotRunTests": true,
        "alwaysBuildAllProjects": true
      }
    }
  ]
}

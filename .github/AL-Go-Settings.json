{
  "type": "AppSource App",
  "templateUrl": "https://github.com/microsoft/AL-Go-AppSource@main",
  "templateSha": "10e466217258cbc3aedec799c01793295c8d5ecb",
  "rulesetFile": "../sr365.ruleset.json",
  "useProjectDependencies": true,
  "alwaysBuildAllProjects": false,
  "enableCodeCop": true,
  "enableUICop": true,
  "enableAppSourceCop": true,
  "failOn": "error",
  "cacheKeepDays": 10,
  "doNotBuildTests": false,
  "doNotRunTests": false,
  "doNotSignApps": false,
  "BcContainerHelperVersion": "preview",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*"
  ],
  "CICDPullRequestBranches": [
    "main"
  ],
  "skipUpgrade": false,
  "ConditionalSettings": [
    {
      "branches": [
        "feature/*"
      ],
      "settings": {
        "doNotSignApps": true,
        "doNotRunTests": true
      }
    },
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "doNotRunTests": true,
        "alwaysBuildAllProjects": true,
        "buildModes": [
          "Default",
          "NextMajor"
        ]
      }
    },
    {
      "buildModes": [
        "NextMajor"
      ],
      "settings": {
        "artifact": "////nextmajor",
        "versioningStrategy": 16,
        "skipUpgrade": true,
        "repoVersion": "27.1",
        "doNotSignApps": false,
        "UpdateDependencies": true,
        "preprocessorSymbols": [
          "NEXTMINOR",
          "NEXTMAJOR"
        ]
      }
    }
  ]
}
